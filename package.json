{
  "name": "jcc-solutions",
  "version": "1.0.0",
  "description": "build frontend bundle",
  "main": "webpack.mix.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "start": "npm run watch",
    "sync": "cross-env MIX_SYNC=1 npm run watch",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.crafton.pl/crafton/jcc-solutions/git"
  },
  "author": "Crafton",
  "license": "crafton",
  "bugs": {
    "url": "https://git.crafton.pl/crafton/jcc-solutions/issues"
  },
  "homepage": "https://git.crafton.pl/crafton/jcc-solutions/README",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-react": "^7.18.6",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.27.11",
    "browser-sync-webpack-plugin": "^2.3.0",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "laravel-mix": "^6.0.49",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "swiper": "^6.8.4"
  },
  "browserslist": [
    "chrome 60",
    "firefox 55",
    "safari 10",
    "edge 15"
  ]
}
